{
    "version": 2,
    "functions": {
        "src/index.js": {
            "runtime": "nodejs20.x",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}